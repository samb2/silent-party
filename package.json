{
  "name": "express-strater",
  "version": "1.0.0",
  "description": "This template is a quick starter for other project.",
  "main": "src/api/bin",
  "scripts": {
    "tsc": "tsc",
    "start": "cross-env NODE_ENV=production ts-node --files src/api/bin",
    "start-dev": "cross-env NODE_ENV=development tsnd --poll --files src/api/bin",
    "start-test": "cross-env NODE_ENV=test tsnd --files src/api/bin",
    "jest": "jest",
    "jest-cover": "jest --coverage",
    "migrate-up": "npx migrate up",
    "migrate-down": "npx migrate down {$migrate-name}",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write **/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samb2/express-strater.git"
  },
  "keywords": [
    "express",
    "typescript",
    "api",
    "microservice"
  ],
  "author": "samb2",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samb2/express-strater/issues"
  },
  "homepage": "https://github.com/samb2/express-strater#readme",
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "auto-bind": "^4.0.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.14.2",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "irolegroup": "^1.0.5",
    "ts-node": "^10.9.1",
    "morgan": "^1.10.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
